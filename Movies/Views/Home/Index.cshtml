					@foreach (var item in Model)
					{
						<div class="card">
                                <a asp-action="GetMovieById" asp-controller="Movie" class="btn" asp-route-id="@item.MovieId">

                                    <img width="200" src="@item.CoverPhoto" />
                                </a>
                                    <h5 class="card-title" style="font-family: 'Anton', sans-serif">@item.MovieName<hr></h5>
                            </div>
					}
				</div>
				
				
				
			</div>
		</div>
	</div>
